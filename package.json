{
  "name": "@cesarecaoduro/occtwasm-core",
  "version": "0.1.2",
  "description": "OpenCascade (OCCT) compiled to WebAssembly with TypeScript bindings",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./TKMath": {
      "import": {
        "types": "./dist/TKMath.d.ts",
        "default": "./dist/TKMath.js"
      },
      "require": {
        "types": "./dist/TKMath.d.cts",
        "default": "./dist/TKMath.cjs"
      }
    },
    "./TKBRep": {
      "import": {
        "types": "./dist/TKBRep.d.ts",
        "default": "./dist/TKBRep.js"
      },
      "require": {
        "types": "./dist/TKBRep.d.cts",
        "default": "./dist/TKBRep.cjs"
      }
    },
    "./TKTopAlgo": {
      "import": {
        "types": "./dist/TKTopAlgo.d.ts",
        "default": "./dist/TKTopAlgo.js"
      },
      "require": {
        "types": "./dist/TKTopAlgo.d.cts",
        "default": "./dist/TKTopAlgo.cjs"
      }
    },
    "./TKGeomBase": {
      "import": {
        "types": "./dist/TKGeomBase.d.ts",
        "default": "./dist/TKGeomBase.js"
      },
      "require": {
        "types": "./dist/TKGeomBase.d.cts",
        "default": "./dist/TKGeomBase.cjs"
      }
    },
    "./helpers": {
      "import": {
        "types": "./dist/helpers.d.ts",
        "default": "./dist/helpers.js"
      },
      "require": {
        "types": "./dist/helpers.d.cts",
        "default": "./dist/helpers.cjs"
      }
    },
    "./occt.wasm": "./dist/occt.wasm"
  },
  "files": [
    "dist",
    "NOTICE",
    "SOURCE_AVAILABILITY.md"
  ],
  "scripts": {
    "codegen": "tsx codegen/src/codegen.ts",
    "build:ts": "tsup",
    "build": "npm run codegen && npm run build:ts && npm run copy-wasm",
    "copy-wasm": "node scripts/copy-wasm.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist bindings/generated"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "yaml": "^2.6.0"
  },
  "keywords": [
    "opencascade",
    "occt",
    "cad",
    "webassembly",
    "wasm",
    "brep",
    "geometry"
  ],
  "license": "LGPL-2.1-only WITH OCCT-exception-1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesarecaoduro/occtwasm.git"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
